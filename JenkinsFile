pipeline {
  agent {
   
  }
  stages {
    stage('Checkout') {
      steps {
        sh 'echo passed'
        
      }
    }
    stage('Build and Test') {
      steps {
        sh 'ls -ltr'
        // build the project and create a JAR file
        sh 'mvn clean package'
      }
    }

    stage('Docker run'){
        steps{
            sh 'docker run hello-world'
        }
    }

  }
}
